{
  "name": "@fifteen/design-system-vue",
  "version": "0.19.1",
  "private": false,
  "description": "Vue 3 (Composition API + Typescript) implementation of the Fifteen Design System",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birotaio/fifteen-design-system-vue.git"
  },
  "keywords": [
    "fifteen",
    "design",
    "system",
    "vue3",
    "composition api",
    "typescript"
  ],
  "author": "Fifteen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/birotaio/fifteen-design-system-vue/issues"
  },
  "homepage": "https://fifteen.eu",
  "types": "dist/types/src/index.d.ts",
  "files": [
    "dist",
    "resolvers.d.ts",
    "rules.d.ts",
    "router.d.ts",
    "icons"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/lib/index.js",
      "require": "./dist/cjs/lib/index.js"
    },
    "./resolvers": {
      "import": "./dist/es/resolvers/index.js",
      "require": "./dist/cjs/resolvers/index.js"
    },
    "./rules": {
      "import": "./dist/es/rules/index.js",
      "require": "./dist/cjs/rules/index.js"
    },
    "./router": {
      "import": "./dist/es/router/index.js",
      "require": "./dist/cjs/router/index.js"
    },
    "./styles/*.styl": "./dist/styles/*.styl",
    "./style.css": "./dist/style.css",
    "./theme.css": "./dist/theme.css",
    "./icons/*.svg": "./icons/*.svg"
  },
  "scripts": {
    "dev": "yarn sb",
    "sb": "storybook dev -p 3003",
    "dev:build": "DEV_BUILD=1 yarn build",
    "build:sb": "zx scripts/build-sb.mjs",
    "build:lib": "vite build",
    "build:types:lib": "vue-tsc -p tsconfig.types.json && tsc-alias -p tsconfig.types.json",
    "build:types:resolvers": "tsc -p tsconfig.resolvers.json --declaration --emitDeclarationOnly --outDir dist/types",
    "build:types:rules": "tsc -p tsconfig.rules.json --declaration --emitDeclarationOnly --outDir dist/types",
    "build:types:router": "vue-tsc -p tsconfig.router.json && tsc-alias -p tsconfig.router.json",
    "build:types": "yarn build:types:lib && yarn build:types:resolvers && yarn build:types:rules && yarn build:types:router",
    "build:theme": "zx scripts/build-theme.mjs && mkdir -p dist/assets/fonts && cp src/assets/fonts/* dist/assets/fonts && mkdir -p dist/styles && cp -r src/styles/* dist/styles",
    "build:icons": "zx scripts/build-icons.mjs",
    "build:resolvers": "vite build --config vite.config.resolvers.ts",
    "build:rules": "vite build --config vite.config.rules.ts",
    "build:router": "vite build --config vite.config.router.ts",
    "build": "zx scripts/build.mjs",
    "lint-staged": "lint-staged",
    "lint": "eslint --ext .ts,.vue .",
    "type-check": "vue-tsc --noEmit",
    "prettier": "prettier --write .",
    "lus": "lus",
    "test": "vitest run",
    "test-dev": "vitest",
    "test-related": "vitest related",
    "coverage": "vitest run --coverage",
    "husky": "husky install",
    "prepush": "yarn husky:pre-commit",
    "release": "npm-release",
    "release:beta": "npm-release --tag beta"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@fifteen/shared-lib": "^0.9.0",
    "@fifteen/tooling": "^0.1.0",
    "@prettier/plugin-pug": "^2.0.0",
    "@qnp/lus": "^0.3.0",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.1.0",
    "@storybook/vue3-vite": "^7.1.0",
    "@types/react": "file:./stubs/types__react",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@typescript-eslint/typescript-estree": "^5.26.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@volar/vue-language-plugin-pug": "^1.0.3",
    "babel-loader": "^8.2.5",
    "camelcase": "^6.3.0",
    "csstype": "^3.1.0",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.0.1",
    "eslint-plugin-vue-pug": "^0.6.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "fast-deep-equal": "^3.1.3",
    "globby": "^13.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "maska": "^1.5.0",
    "node-html-parser": "^5.3.3",
    "prettier": "^2.6.2",
    "pug": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "storybook": "^7.1.0",
    "stylus": "^0.58.0",
    "stylus-supremacy": "^2.15.0",
    "tsc-alias": "^1.6.8",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.0.4",
    "vite-svg-loader": "^3.3.0",
    "vitest": "^0.13.0",
    "vue-tsc": "^1.0.9",
    "vue3-popper": "https://github.com/qnp/vue3-popper#build",
    "zx": "^6.1.0"
  },
  "dependencies": {
    "@vee-validate/rules": "^4.10.5",
    "@vueuse/core": "^10.2.1",
    "commander": "^9.4.1",
    "libphonenumber-js": "^1.10.6",
    "vee-validate": "4.10.5",
    "vue": "^3.3.4",
    "vue-router": "^4.1.6",
    "xss": "^1.0.14"
  }
}
